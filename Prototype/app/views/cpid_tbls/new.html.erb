<% breadcrumb :new_cpid, @base_cmds %>
<h1>CREATE CPID_TBL </h1>

<div class="perfect-forms">
<%= form_for(@cpids) do |f| %>

  <%= render 'shared/error_messages', object: f.object %>
  <%= f.hidden_field :CMD_ID, :value => @cpids.CMD_ID %>

<div class="form-row">
  <%= f.label :CPID %>
  <%= f.text_field :CPID, class: 'form-control' %>
  <div id="details_holder" data-original_1="<%=@cpids.CMD_ID%>" data-original_2="<%=@cpids.CPID%>"></div>
  </div>

<div class="form-row">
  <%= f.label :CPID_DESC%>
  <%= f.text_field :CPID_DESC, class: 'form-control' %>
</div>

<div class="form-row">
  <%= f.label :PROC_TYPE%>
  <%= f.collection_select(:PROC_TYPE, LKUP_TBL_TYPE_TBL.all, :LKUP_TBL_TYPE, :LKUP_TBL_TYPE, {:prompt => "Please select"},{class: 'form-control'})%>
</div>

<div class="form-row">
  <%= f.label :TM_VAL %>
  <%= f.text_field :TM_VAL, class: 'form-control' %>
  <div class="value_error_text"></div>
</div>

<div class="form-row">
  <%= f.label :TM_COUNT %>
  <%= f.text_field :TM_COUNT, class: 'form-control' %>
</div>

  <div class="form-row">
  <%= f.submit 'Create', class: "btn btn-primary" %>
  </div>
<% end %>
</div>
