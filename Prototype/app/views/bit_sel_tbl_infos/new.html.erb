<% if session[:cmd_id]==0 %>
	<% breadcrumb :bit_info_new, @bit_infos %>
<% else %>
	<% breadcrumb :new_bit_info, @base_cmds %>
<% end %>
<h1>CREATE BIT_SEL_TBL_INFO </h1>
<div class="perfect-forms">
<%= form_for(@bit_infos) do |f| %>
<%= render 'shared/error_messages', object: f.object %>

<div class="form-row">
  <%= f.label :BIT_SEL_TBL_NO %>
  <%= f.text_field :BIT_SEL_TBL_NO, class: 'form-control' %>
  <div id="details_holder" data-original_1="<%=@bit_infos.BIT_SEL_TBL_NO%>"></div>
</div>

<div class="form-row">
  <%= f.label :BIT_SEL_TBL_NAME %>
  <%= f.text_field :BIT_SEL_TBL_NAME, class: 'form-control' %>
</div>

<div class="form-row">
  <%= f.submit 'Create', class: "btn btn-primary" %>
</div>

<% end %>
</div>